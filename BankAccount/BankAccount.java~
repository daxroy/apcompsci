public class BankAccount
{
  private String name;
  private int balance;
  private double interest;
  public BankAccount() {
    name = "Unknown";
    balance = 0;
    interest = 0.001;
 }
  
  public BankAccount(String n, int b) {
    name = n;
    balance = b;
    interest = 0.001;
 }
  
  public BankAccount(String n, int b, double i) {
    name = n;
    balance = b;
    interest = i;
 }
}